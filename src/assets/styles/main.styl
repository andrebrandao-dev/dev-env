@import "reset.styl"
@import "helpers/**/*.styl"
@import "components/**/*.styl"

body
  font-family 'Open Sans', sans-serif
  max-width 100%
  min-height 100vh
  height auto
  overflow-x hidden
  overflow-y auto
  background-color #FFF
  color #333
  font-size 14px

a
  text-decoration none
  color #333

p + p
  margin-top 15px
